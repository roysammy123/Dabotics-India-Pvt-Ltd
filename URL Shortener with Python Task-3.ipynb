   "source": [
    "import pyshorteners\n",
    "\n",
    "def shorten_url(url):\n",
    "    s = pyshorteners.Shortener()\n",
    "    return s.tinyurl.short(url)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    url_to_shorten = input(\"Enter the URL to be shortened: \")\n",
    "    print(\"Shortened URL:\", shorten_url(url_to_shorten))\n"
   ]
  }
